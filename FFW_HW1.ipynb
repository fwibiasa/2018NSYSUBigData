   "source": [
    "# Quadratic values in a list with loops\n",
    "list = [1,2,3,4]\n",
    "count = 0\n",
    "while (count < 4):\n",
    "    for value in list:\n",
    "        list[count] = value**2\n",
    "        count += 1\n",
    "\n",
    "print(\"QuadraticList =\",list)"
   ]
  }
